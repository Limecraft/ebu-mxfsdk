chemas/434/Groups/PropertyRequiredStatus">D/req</xs:appinfo>							<xs:appinfo source="http://www.smpte-ra.org/schemas/434/Groups/LocalTag">3F.10</xs:appinfo>						</xs:annotation>					</xs:element>				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<!-- - -->	<!-- - -->	<!-- - -->	<!-- - -->	<xs:element name="IndexTable">		<xs:complexType>			<xs:sequence>				<xs:element ref="s377mGroups:IndexTableSegment" minOccurs="0" maxOccurs="unbounded"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<!-- - -->	<!-- - --></xs:schema>