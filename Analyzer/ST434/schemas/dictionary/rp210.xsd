	<!-- - -->
	<!-- - -->
	<xs:element name="VC1MaxGOP" type="s377mTypes:UInt16">
		<xs:annotation>
			<xs:documentation>Specifies the maximum occurring spacing between I frames. A value of 0
				or the absence of this property implies no limit to the maximum
				GOP.</xs:documentation>
			<xs:appinfo source="http://www.smpte-ra.org/schemas/434/Dictionary/ElementName">VC-1 Max
				GOP</xs:appinfo>
			<xs:appinfo source="http://www.smpte-ra.org/schemas/434/Dictionary/Length">2</xs:appinfo>
			<xs:appinfo source="http://www.smpte-ra.org/schemas/434/Dictionary/ElementUL">06.0E.2B.34.01.01.01.10.04.01.06.04.05.00.00.00</xs:appinfo>
			<xs:appinfo source="http://www.smpte-ra.org/schemas/434/Dictionary/DataElementDefinition"/>
		</xs:annotation>
	</xs:element>
	<!-- - -->
	<!-- - -->
	<!-- - -->
	<xs:element name="VC1BPictureCount" type="s377mTypes:UInt16">
		<xs:annotation>
			<xs:documentation>Specifies the maximum number of B pictures between P or I frames.
				BI-pictures count as B-pictures and skipped-pictures count as
				P-pictures</xs:documentation>
			<xs:appinfo source="http://www.smpte-ra.org/schemas/434/Dictionary/ElementName">VC-1
				B-Picture Count</xs:appinfo>
			<xs:appinfo source="http://www.smpte-ra.org/schemas/434/Dictionary/Length">2</xs:appinfo>
			<xs:appinfo source="http://www.smpte-ra.org/schemas/434/Dictionary/ElementUL">06.0E.2B.34.01.01.01.10.04.01.06.04.06.00.00.00</xs:appinfo>
			<xs:appinfo source="http://www.smpte-ra.org/schemas/434/Dictionary/DataElementDefinition"/>
		</xs:annotation>
	</xs:element>
