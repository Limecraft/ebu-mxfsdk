	<!-- - -->
	<!-- - -->
	<xs:element name="XTsiz" type="s377mTypes:UInt32">
		<xs:annotation>
			<xs:documentation>Width of one reference tile with respect to the reference grid, as
				defined in ISO/IEC 15444-1 Annex A.5.1.</xs:documentation>
			<xs:appinfo source="http://www.smpte-ra.org/schemas/434/Dictionary/ElementName">XTsiz</xs:appinfo>
