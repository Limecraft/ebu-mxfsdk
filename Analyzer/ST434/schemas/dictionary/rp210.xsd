	<!-- - -->
	<!-- - -->
	<xs:element name="VC1AverageBitRate" type="s377mTypes:UInt32">
		<xs:annotation>
			<xs:documentation>Average bit rate of VC-1 video elementary stream in
				bit/s.</xs:documentation>
