	<!-- - -->
	<!-- - -->
	<xs:element name="YTsiz" type="s377mTypes:UInt32">
		<xs:annotation>
			<xs:documentation>Vertical offset from the origin of the reference grid to the top side
				of the first tile, as defined in ISO/IEC 15444-1 Annex A.5.1</xs:documentation>
			<xs:appinfo source="http://www.smpte-ra.org/schemas/434/Dictionary/ElementName">YTsiz</xs:appinfo>
			<xs:appinfo source="http://www.smpte-ra.org/schemas/434/Dictionary/Length">4
				bytes</xs:appinfo>
			<xs:appinfo source="http://www.smpte-ra.org/schemas/434/Dictionary/ElementUL">06.0E.2B.34.01.01.01.0A.04.01.06.03.07.00.00.00</xs:appinfo>
			<xs:appinfo source="http://www.smpte-ra.org/schemas/434/Dictionary/DataElementDefinition"/>
